{
  "name": "goldy-quote-server",
  "version": "1.0.0",
  "description": "Backend server for GoldyQuote insurance comparison",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "pretest": "node compile-tests.js",
    "test": "playwright test -c tests-out",
    "test:ui": "playwright test --ui -c tests-out",
    "test:debug": "playwright test --debug -c tests-out",
    "test:mcp": "npm run build && node test-mcp-integration.js",
    "docker-build": "docker build -t goldy-quote-server ."
  },
  "dependencies": {
    "@playwright/test": "^1.53.1",
    "@types/eventsource": "^3.0.0",
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "eventsource": "^4.0.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "playwright": "^1.53.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.10",
    "@types/ws": "^8.5.10",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3"
  }
}